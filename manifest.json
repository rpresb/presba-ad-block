{
    "name": "adblock",
    "version": "1.0",
    "description": "AdBlock",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://multicanais.com/*",
                "*://www.superflix.net/*",
                "*://futemax.fm/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}